{
  "env": {
    "browser": true
  },
  "globals": {
    "graphql": false
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
        "arrowFunctions": true,
        "jsx": true,
        "experimentalObjectRestSpread": true,
        "destructuring": true,
        "spread": true
    }
}
}